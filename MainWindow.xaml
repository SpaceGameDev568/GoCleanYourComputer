<Window x:Class="GoCleanYourComputer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoCleanYourComputer"
        mc:Ignorable="d"
        Title="GoCleanYourComputer" Height="800" Width="600">
    <Grid>
        <TextBox x:Name="TextBox1" HorizontalAlignment="Center" VerticalAlignment="Top">Hello World!</TextBox>
        <Button x:Name="CleanButton" VerticalAlignment="Center" HorizontalAlignment="Center" Click="CleanButton_Click">Clean</Button>
        <Button x:Name="RMTempFilesButton" Width="200px" Height="50px" VerticalAlignment="Top" HorizontalAlignment="Center" Click="RMTempFiles_Click">Remove Temporary Files</Button>
    </Grid>
</Window>
